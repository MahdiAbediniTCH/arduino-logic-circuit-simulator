<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@../CSS/Style.css" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.group24.truthtable.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Label alignment="CENTER" contentDisplay="CENTER" layoutX="134.0" layoutY="14.0" prefHeight="74.0" prefWidth="311.0" text="Welcome to logic circuit simulator">
        <font>
            <Font name="Roboto Slab SemiBold" size="18.0" />
        </font></Label>
    <VBox alignment="CENTER" layoutX="200.0" layoutY="102.0" prefHeight="243.0" prefWidth="179.0" spacing="20.0">
        <children>
            <Button mnemonicParsing="false" text="Add Table" fx:id="addTable" onMouseClicked="#goToAddTableMenu"/>
            <Button mnemonicParsing="false" text="Current Tables" fx:id="currentTable" onMouseClicked="#goToCurrentTableMenu" />
            <Button mnemonicParsing="false" text="Export" fx:id="export" />
        </children>
    </VBox>
</AnchorPane>
